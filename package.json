{
  "name": "train-faces-cognitive",
  "version": "0.1.0",
  "description": "UI for training faces on Microsoft Cognitive Services",
  "main": "index.js",
  "engines": {
    "node": ">=6.9.0",
    "npm": "^4.0.0"
  },
  "theme": "./theme.js",
  "scripts": {
    "clean": "rimraf dist",
    "compile": "better-npm-run compile",
    "lint": "eslint bin build config server src tests",
    "lint:fix": "npm run lint -- --fix",
    "start": "better-npm-run start",
    "serve": "better-npm-run serve",
    "test": "better-npm-run test",
    "test:dev": "npm run test -- --watch",
    "deploy": "better-npm-run deploy",
    "deploy:dev": "better-npm-run deploy:dev",
    "deploy:prod": "better-npm-run deploy:prod",
    "codecov": "cat coverage/*/lcov.info | codecov"
  },
  "betterScripts": {
    "compile": {
      "command": "node bin/compile",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "serve": {
      "command": "nodemon server --ignore dist --ignore coverage --ignore tests --ignore src",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "deploy": {
      "command": "npm run lint && npm run clean && npm run compile",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "deploy:dev": {
      "command": "npm run deploy",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "deploy:prod": {
      "command": "npm run deploy",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "start": {
      "command": "node bin/dev-server",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "test": {
      "command": "node ./node_modules/karma/bin/karma start config/karma.config",
      "env": {
        "NODE_ENV": "test",
        "DEBUG": "app:*"
      }
    }
  },
  "author": "Hao Luo",
  "license": "MIT",
  "dependencies": {
    "azure-storage": "^2.0.0",
    "better-npm-run": "0.0.13",
    "body-parser": "^1.17.1",
    "compression": "^1.6.2",
    "css-loader": "^0.26.0",
    "cssnano": "^3.7.4",
    "debug": "^2.2.0",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^1.0.0",
    "file-loader": "^0.9.0",
    "fs-extra": "^1.0.0",
    "html-webpack-plugin": "^2.22.0",
    "imports-loader": "^0.7.0",
    "ip": "^1.1.2",
    "js-md5": "^0.4.2",
    "json-loader": "^0.5.4",
    "lodash": "^4.17.4",
    "node-fetch": "^1.6.3",
    "node-sass": "^4.0.0",
    "normalize.css": "^5.0.0",
    "project-oxford": "^1.7.0",
    "query-string": "^4.3.2",
    "react": "^15.0.0",
    "react-redux": "^5.0.1",
    "react-router": "^3.0.0",
    "redux": "^3.6.0",
    "rimraf": "^2.5.4",
    "uuid": "^3.0.1",
    "webpack": "^1.12.14",
    "yargs": "^6.3.0"
  },
  "devDependencies": {
    "antd": "^2.7.1",
    "babel-core": "^6.17.0",
    "babel-eslint": "^7.1.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-import": "^1.1.0",
    "babel-plugin-istanbul": "^3.0.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.3.13",
    "babel-runtime": "^6.11.6",
    "base64-arraybuffer": "^0.1.5",
    "brace": "^0.10.0",
    "chai": "^3.4.1",
    "chai-as-promised": "^6.0.0",
    "chai-enzyme": "^0.6.1",
    "cheerio": "^0.22.0",
    "codecov": "^1.0.1",
    "enzyme": "^2.0.0",
    "eslint": "^3.0.1",
    "eslint-config-standard": "^6.0.0",
    "eslint-config-standard-react": "^4.0.0",
    "eslint-plugin-babel": "^4.0.0",
    "eslint-plugin-promise": "^3.0.0",
    "eslint-plugin-react": "^6.0.0",
    "eslint-plugin-standard": "^2.0.0",
